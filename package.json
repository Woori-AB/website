  "engines": {
    "node": ">=24",
    "npm": ">=11"
  },
  "scripts": {
    "dev": "next dev",
    "prebuild": "next telemetry disable",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "vitest __tests__/unit --run",
    "test:unit:watch": "vitest __tests__/unit",
    "test:unit:coverage": "vitest __tests__/unit --reporter=verbose --coverage --run",
    "test:unit:ui": "vitest __tests__/unit --ui --coverage",
    "test:integration": "vitest __tests__/integration --run",
    "test:integration:watch": "vitest __tests__/integration",
    "test:e2e": "npm run build && npx playwright test",
    "verify": "npm run lint && npm run test:unit:coverage && npm run test:e2e"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.0.1",
    "babel-plugin-react-compiler": "^1.0.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "next": "^16.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-intl": "^8.0.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^4.0.0",
    "@vitest/ui": "^4.0.0",
    "eslint": "^9.0.0",
    "eslint-config-next": "^16.0.0",
    "jsdom": "^27.0.0",
    "postcss": "^8",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.0"
  }
}
